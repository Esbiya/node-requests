{
  "name": "@esbiya/requests",
  "version": "1.0.1",
  "description": "a network requests lib",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "directories": {
    "src": "src",
    "test": "test"
  },
  "scripts": {
    "build": "tsc",
    "test": "jest --verbose",
    "test-c": "jest --coverage",
    "release": "tsc && npm publish"
  },
  "jest": {
    "rootDir": "test",
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.tsx?$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  },
  "author": "esbiya",
  "license": "MIT",
  "dependencies": {
    "http-proxy-agent": "^2.1.0",
    "https-proxy-agent": "^2.2.1",
    "iconv-lite": "^0.6.2",
    "request": "^2.88.0",
    "socks-proxy-agent": "^4.0.2",
    "source-map-support": "^0.5.11",
    "typescript": "^4.2.4",
    "jest": "^26.6.3",
    "ts-jest": "^26.5.6"
  },
  "devDependencies": {
    "@types/node": "^12.20.12",
    "@types/request": "^2.48.1",
    "@types/tough-cookie": "^4.0.0",
    "@types/cheerio": "^0.22.28",
    "@types/iconv-lite": "^0.0.1"
  }
}
